              <MenuItem fx:id="idCadastrar2" mnemonicParsing="false" onAction="#cadastrarAluno" text="Cadastrar Novo" />
                  <MenuItem fx:id="idCa" mnemonicParsing="false" onAction="#gerenciarAluno" text="Gerenciamento de Dados" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Professores">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#cadastrarProfessor" text="Cadastrar Novo" />
