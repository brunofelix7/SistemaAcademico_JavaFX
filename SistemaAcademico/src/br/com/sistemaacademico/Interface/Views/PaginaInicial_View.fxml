<AnchorPane id="AnchorPane" minHeight="768.0" minWidth="1366.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.sistemaacademico.Interface.Controllers.PaginaInicial_Controller">
   <children>
      <MenuBar minWidth="1366.0" prefHeight="25.0" prefWidth="1366.0">
        <menus>
          <Menu mnemonicParsing="false" text="Arquivo">
            <items>
                  <MenuItem fx:id="idRelatorio" mnemonicParsing="false" onAction="#relatorio" text="Gerar Relatório" />
              <MenuItem fx:id="idSair" mnemonicParsing="false" onAction="#sair" text="Sair" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Alunos">
